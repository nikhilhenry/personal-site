{
  "routes": [
    {
      "src": "/api/hello",
      "dest": "/api/hello.py"
    }
  ],
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder",
      "config": {
        "serverFiles": [".nuxt/content/**", "content/**","api/**"],
        "generateStaticRoutes" : true
      }
    }
  ],
  "functions":{
    "api/**/*.py": {
      "excludeFiles": "public/**"
    }
  }
}
